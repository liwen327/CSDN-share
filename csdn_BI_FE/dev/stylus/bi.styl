        padding-bottom 0
        .page_src
            overflow-x auto
            overflow-y hidden
            white-space:nowrap
            .level_box
                display inline-block
                width 267px
                .lev_top
                    h5
                        color #333333
                        font-size 12px
                        height 15px
                        line-height 15px
                        i
                            padding-left 5px
                            color #aeaeae
                            font-size 13px
                    span
                        font-size 11px
                        color #adadad
                .lev_chart
                    height 200px
                    margin-top 15px
                    i
                        padding 5px 10px
                    .chart_l
                        list-style none
                        margin 0
                        padding 0
                        float left
                        width 182px
                        height 200px
                        .lev0_l
                            float left
                            width 150px
                            height 200px
                            background #bee7e9
                            span
                                color #676767
                                font-size 15px
                        .lev0_r
                            float right
                            width 32px
                            height 200px
                            .lev0_r_t
                                width 32px
                                height 10px
                                background #d8e4e4
                            .lev0_r_b
                                width 15px
                                height 190px
                                line-height 190px
                                text-align center
                                background -webkit-gradient(linear, 0 0, 0 100%, from(#e6ceac), to(#fefdfb))
                                background -moz-linear-gradient(top, #e6ceac,#fefdfb)
                                /*filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#e6ceac,endColorstr=#fefdfb,grandientType=0)*/
                                i
                                    color #ffffff
                                    padding 0
                        li
                            height 25px
                            line-height 20px
                            border-top 5px solid #bee7e9
                            border-right 3px solid #e6ceac
                            margin-bottom 15px
                            span
                                color #676767
                                font-size 15px
                    .chart_r
                        float right

            .direction
                display inline-block
                width 100px
                .dir
                    margin 88px 0
                    .go
                        width 50px
                        border 1px dashed #d2d9df
                        margin 0 auto
                        text-align center
                        i
                            color #d4d9df
                            font-size 30px
                            line-height 50px
                            height 50px
                    .add
                        width 50px
                        margin 0 auto
                        i
                            text-align center
                            color #c8c8c8
                            font-size 13px
                            padding-right 5px
                            line-height 20px
                        a
                            color #3697c4
                            font-size 13px

