    .data_grid,.data_cusrep
        background #ffffff
        border 1px solid #d1d9dc
        .table
            thead tr th,tbody tr td
                text-align center
                font-size 10px
            thead tr
                height 40px
                background #3897c5
                color #ffffff
            tbody tr
                color #333333
    .data_cusrep
        .table
            thead tr th,tbody tr td
                text-align left
                padding-left 60px
                width 10%
            thead tr th
                &:first-child
                    padding-left 130px
            tbody tr td
                input[type="checkbox"]
                    margin-right 30px
                a
                    text-decoration none
                    color #323232
                    &:hover
                        color #1b6d85
                span
                    border 1px solid #e4e4ee
                    margin-left 13px
                    &:first-child
                        margin-left 26px
                    i
                        width 20px
                        height 20px
                        text-align center
                        vertical-align middle
                        padding-top 2px

    .chose,.operation,.data_conf
        margin-top 10px
    .chose,.operation
        select,.btn
            width 180px
            height 40px
            margin 4px 0
            margin-right 20px
        select
            border 1px solid #e1e4ed
            border-radius 4px
            padding-left 5px
    .operation
        margin-bottom 15px
        .btn
            text-align center
            line-height 30px
            position relative
            .fa-close
                position absolute
                color #aeaeae
                font-size 13px
                width 18px
                height 18px
                border-radius 50%
                background #ffffff
                top 4px
                right 5px
                padding 2px 3px
                box-shadow 1px 0 1px #fff
                -webkit-box-shadow 1px 0 1px #fff
                -moz-box-shadow 1px 0 1px #fff
            .fa-plus
                color #c2c2c2
                padding 5px
                font-size 20px
            &:nth-child(1)
                background #1fb5ac
            &:nth-child(2)
                background #9972b5
            &:nth-child(3)
                background #feb45d
            &:last-child
                background #ffffff
                width 45px
                border 1px dashed #dcdcda
                padding 5px

    .sel_group
        margin-left 10px
        select,input,.btn
            background #f1f2f7
            height 30px
        select
            width 135px
            margin-right 30px
            border 1px solid #e4e4ec
            border-radius 2px
            -webkit-border-radius 2px
            -moz-border-radius 2px
            color #313131
            font-size 12px
        input[type="text"]
            width 200px
            background #f1f2f7
            border 1px solid #e3e4e9
        .btn
            padding 4px 10px
            span
                color #979998
                font-size 16px


    .condition
        margin-left 10px
        margin-top 10px
        color #acacac
        font-size 15px
    .data_conf
        .btn
            width 70px
            height 30px
            margin 0 25px 0 0
            font-size 12px
            &:nth-child(1)
                background #33414e
                color #fffefb
            &:nth-child(2)
                background #f1f2f7
                color #333333
                border 1px solid #d4d9dd
            .fa-check
                color #1fc71c
            .fa-close
                color #ef5152

    .data_export
        padding-top 40px
        font-size 12px
        span
            color #333331
            vertical-align middle
        .btn
            height 30px
            margin-left 15px
