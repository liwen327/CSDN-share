/******************** base style **********************/
*
    margin 0
    padding 0
body
    font 14px/1.8 'microsoft yahei'
    background #f1f2f7
$setOvf()
    white-space nowrap
    overflow hidden
    text-overflow ellipsis
$row = 0px
.row
    margin-left -($row)
    margin-right -($row)
    [class^="col-"]
        padding-left $row
        padding-right $row
/***************** variable **************************/
$a_hover_bg = #3d4f5d
$l_active_bg = #232f3b
$a_h_color = #3897c5


/***************** scdn-toolbar **************************/
.csdn-toolbar
    position fixed
    width 100%
    top 0
    z-index 9
    .container
        width auto
        .logo .img
            background none
            &:before
                content "BI"
                font-size 20px
                line-height 40px
                color #fff

/******************* left nav ************************/
.left-nav
    position fixed
    width 240px
    height 100%
    top 40px
    background #33414e
    color #fff
    ul
        font-size 16px
        li
            line-height 40px
            border-bottom 1px solid #2e3c47
            &.active
                background $l_active_bg
                color $a_h_color
            a
                display block
                padding 5px 10px 5px 34px
                color #ffffff
                cursor pointer
                &:hover,&.active
                    background $a_hover_bg
                    text-decoration none
                    i
                        color $a_h_color
                    span i
                        color #fff
                i
                    padding-right 20px
                span
                    display inline-block
                    float right
            ul
                font-size 14px
                padding-left 20px
                display none
                span
                    float right
                a
                    &:hover
                        color $a_h_color
                    &.active
                        color $a_h_color
                        background #232f3b
/******************* content top bar ************************/
.content_bar
    background #ffffff
    padding: 40px 10px 0 240px;
    ul,ol
        margin 8px 0 0
    ul
        overflow hidden
        li
            float left
            padding 0 30px
            a
                color #5d5d5d
                padding 13px 23px
                line-height 52px
                font-size 16px
                text-decoration none
                &:hover
                    border-bottom 3px solid #3897c5
            &.active
                a
                    border-bottom 3px solid #3897c5


    ol
        li
            display inline-block
            padding 14px 0
            color #999999
            +li
                &:before
                    content ">"
                    padding 0 3px
            a
                font-size 6px
                text-decoration none
                color inherit

/******************* content wrap ************************/
.conwrap
    padding-left 240px
    padding-bottom 40px
.noShow
    display none
/******************* btn & btn group ************************/
.btn
    background #33414e
    color #fff
    padding 4px 15px
    margin-left 20px
    i
        padding-left 10px
.btn-group
    margin-right 22px
    &:last-child
        margin-right 0
    .btn
        background #f1f2f7
        color #333333
        padding 4px 15px
        font-size 12px
        /*width 100px*/
.clearfix
    &:after
        width 0
        height 0
        display block
        content "."
        clear both
        visibility hidden

.content
    padding-right 9px
    overflow-x hidden
    .tit,.data_chart,.data_grid,.data_export,.data_cusrep,.chose,.operation,.data_conf
        margin-left 40px
    .tit
        margin-top 10px
        h4
            font-size 23px
            line-height 30px
            color #323f4f
            i
                padding-right 18px
        .btn
            height 30px
            margin 10px 0
            a
                color #ffffff
                text-decoration none
                &:hover
                    color #d58512

    hr
        background #e3e3ed
        height 2px
        margin-top 0

    .data_chart
        border 1px solid #d3d8de
        .panel-heading
            height 40px
            font-size 15px
            background #3897c5
            color #fff
            line-height 40px
            padding 0 15px
            .ri_tit
                position relative
                i
                    padding 0 5px
                    color #ffffff
                .txt
                    margin-right 25px
                .hide_ico
                    display block
                    position absolute
                    top 15px
                    right 0px
                    width 0
                    height 0
                    border-left 6px solid #fff
                    border-top 6px solid transparent
                    border-bottom 6px solid transparent
                    border-right 6px solid transparent
                .show_ico
                    display block
                    position absolute
                    top 18px
                    right 0px
                    width 0
                    height 0
                    border-top 6px solid #fff
                    border-left 6px solid transparent
                    border-right 6px solid transparent
                    border-bottom 6px solid transparent
                .time_setting
                    display none
                    position absolute
                    top 40px
                    right -15px
                    /*width 100%*/
                    /*height 200px*/
                    border-bottom 1px solid #dadada
                    border-left 1px solid #dadada
                    background #f7f7f7
                    color #5b5b5b
                    overflow hidden
                    z-index 1
                    padding 0 15px
                    .dur_set,.is_apply
                        padding 15px 0
                    .dur_set
                        border-bottom 1px dashed #666666
                        .duration
                            color #5b5b5b
                            label
                                padding-right 20px
                                font-weight 100
                            select
                                width 150px
                        .timeset
                            input
                                width 100px
                                &:first-child
                                    margin-right 20px
                                &:last-child
                                    margin-left 20px
                    .is_apply
                        .btn
                            background transparent
                            margin 0px
                            &:first-child
                                border 1px solid #adb2b5
                                color #6d6d6f
                            &:last-child
                                color #999997
            .sel_chart
                button
                    width 100px
                    border 0
                    background transparent
                    text-align left
                    .txt
                        color #fefefe
                        padding-right 10px
                    .caret
                        color #ffffff
                ul li a
                    color #3897c5


    .panel-body
        .chart-tit
            margin-bottom 10px
        label
            font-size 12px
            color #7d7d7d
            font-weight normal
            padding-right 5px
        .timer_group
            padding-right 4px
            .btn
                padding 4px 7px
                background #f1f2f7
                height 30px
                span
                    color #979998
                    font-size 16px
        .chart
            height 400px

        .pages>.items_l,.pages>.items_m,.pages>.items_r,.lines{
            float:left;
        }
        .pages ul li
            list-style none
            width 100px
            height 30px
            color #fff4f4
            margin 15px 0
            text-align center
            font-size 14px
            padding 2px
        .items_l ul li
            background #b6a2df
        .items_m ul li
            background #fa8564
        .items_r ul li
            background: #1fb5ac



        /* 转化漏斗页面style */
        .funnel_box
            width 100%
            .left,.mid,.right
                display inline-block
            .left,.right
                width 20%
            .mid
                width 30%
            .shop_desc
                .left, .mid, .right
                    vertical-align bottom
                .left span, .right span
                    color #222222
                    font-size 20px
                    font-weight bold
                .left
                    padding-right 10px
                    text-align right
                .mid
                    .left_icon,.tips,.right_icon
                        float left
                    .left_icon
                        width 10%
                        i
                            font-size 50px
                            color #9cdf8f
                            padding 65px 10px 0
                    .tips
                        width 80%
                        border 1px solid #dddddd
                        text-align center
                        span
                            font-size 20px
                            &:nth-child(1)
                                display block
                                margin-top 10px
                                color #212121
                            &:nth-child(2)
                                color #202135
                                font-weight bold
                            &:nth-child(3)
                                color #5f4a75
                        .data_bar
                            width 90%
                            margin 5px auto 20px
                            .bar_l
                                display inline-block
                                height 10px
                                background #9de090
                            .bar_r
                                display inline-block
                                height 10px
                                background #e06b7b

                    .right_icon
                        width 10%
                        text-align right
                        i
                            font-size 50px
                            color #e16c7e
                            padding 65px 10px 0
                .right
                    padding-left 10px
                    text-align left
            .shop_data
                .left, .mid, .right
                    vertical-align top
                .left
                    padding-right 10px
                    text-align right
                    table tr td
                        &:first-child  label
                            float left
                            color #5b5b5b
                .mid
                    position relative
                    .funnel_bg
                        width 50%
                        height 240px
                        margin 0 auto
                    .data_per
                        position absolute
                        top 65px
                        left 25%
                        width 50%
                        text-align center
                        span
                            font-size 17px
                            &:nth-child(1)
                                display block
                                color #232122
                                font-weight bold
                            &:nth-child(1)
                                color #666666
                .right
                    text-align right
            .pay_success .shop_desc .mid .tips span:last-child
                font-size 17px







    /* 页面路径样式 */
